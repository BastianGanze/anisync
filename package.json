{
  "name": "weebsync",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build-app": "esbuild src/index.ts --platform=node --bundle --outfile=build/out.js",
    "build-executables": "pkg -c pkg.json build/out.js --options=no-deprecation -o dist/weebsync",
    "build": "yarn run build-app && yarn run build-executables"
  },
  "devDependencies": {
    "@types/ftp": "^0.3.33",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.8",
    "@types/progress-stream": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "esbuild": "^0.14.11",
    "eslint": "^8.6.0",
    "pkg": "^5.5.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "ftp": "https://github.com/icetee/node-ftp.git",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "progress-stream": "^2.0.0",
    "strongly-typed-events": "^2.1.14",
    "ts-pattern": "^3.3.4",
    "typescript": "^4.5.4"
  }
}
